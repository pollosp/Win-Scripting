'' Check clock difference between to machinesstrComputer = "ZZZZZZ" Set objWMIService = GetObject("winmgmts:\\" & strComputer & "\root\cimv2")Set colItems = objWMIService.ExecQuery("Select * from Win32_OperatingSystem") For Each objItem in colItems    dtmLocalTime = objItem.LocalDateTime    dtmMonth = Mid(dtmLocalTime, 5, 2)    dtmDay = Mid(dtmLocalTime, 7, 2)    dtmYear = Left(dtmLocalTime, 4)    dtmHour = Mid(dtmLocalTime, 9, 2)    dtmMinutes = Mid(dtmLocalTime, 11, 2)    dtmSeconds = Mid(dtmLocalTime, 13, 2)Next dtmNewDate = dtmMonth & "/" & dtmDay & "/" & dtmYeardtmNewTime =  dtmHour & ":" & dtmMinutes & ":" & dtmSeconds   strComputer2 = "YYYYYY" Set objWMIService = GetObject("winmgmts:\\" & strComputer2 & "\root\cimv2")Set colItems = objWMIService.ExecQuery("Select * from Win32_OperatingSystem") For Each objItem in colItems    dtmLocalTime1 = objItem.LocalDateTime    dtmMonth1 = Mid(dtmLocalTime, 5, 2)    dtmDay1 = Mid(dtmLocalTime, 7, 2)    dtmYear1 = Left(dtmLocalTime, 4)    dtmHour1 = Mid(dtmLocalTime, 9, 2)    dtmMinutes1 = Mid(dtmLocalTime, 11, 2)    dtmSeconds1 = Mid(dtmLocalTime, 13, 2)Next dtmNewDate1 = dtmMonth1 & "/" & dtmDay1 & "/" & dtmYear1dtmNewTime1 =  dtmHour1 & ":" & dtmMinutes1 & ":" & dtmSeconds1  Wscript.Echo "ZZZZ " & dtmNewDate & " " & dtmNewTime & " YYYY " & dtmNewDate1 & " " & dtmNewTime1