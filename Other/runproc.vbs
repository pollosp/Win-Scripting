''Execute procces in remote machine via WMIstrComputer = "XXXX"Set objWMIService = GetObject _    ("winmgmts:\\" & strComputer & "\root\cimv2:Win32_Process") Error = objWMIService.Create("E:\XXXXXX", null, null, intProcessID)If Error = 0 Then    Wscript.Echo "Jmeter-server was started with a process ID of " _         & intProcessID & "."Else    Wscript.Echo "Jmeter-server could not be started due to error " & _        Error & "."End If